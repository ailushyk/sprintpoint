datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}


generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  email     String?  @unique
  username  String?
  avatar    String?
  type      String
  theme     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}